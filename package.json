{
  "name": "@liliana-sanfilippo/bibtex-ts-parser",
  "version": "2.0.7",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/liliana-sanfilippo/bibtex-ts-parser",
  "description": "A BitTeX parser implemented in TypeScript (ESNext). Transforming a BibTeX file to an object in memory or a semi-structured file on disk. Based on https://github.com/yepengding/bibtex-js-parser",
  "main": "./dist/cjs/bibtex-ts-parser.js",
  "browser": "./dist/umd/bibtex-ts-parser.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "webpack --config webpack.config.dev.js",
    "build-commonjs": "LIB_TYPE=commonjs webpack --config webpack.config.prod.js",
    "build-umd": "LIB_TYPE=umd webpack --config webpack.config.prod.js",
    "build-module": "LIB_TYPE=module webpack --config webpack.config.prod.js",
    "build": "npm run build-umd && npm run build-commonjs && npm run build-module",
    "test": "npm run dev && mocha --require source-map-support/register dev/bibtex-ts-parser-dev.js",
    "build-debug": "webpack --config webpack.config.prod.js --mode development --devtool source-map"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/bibtex-ts-parser.js",
      "import": "./dist/module/bibtex-ts-parser.js",
      "browser": "./dist/umd/bibtex-ts-parser.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": [
    "bibtex",
    "parser"
  ],
  "author": "Liliana Sanfilippo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liliana-sanfilippo/bibtex-ts-parser"
  },
  "bugs": {
    "url": "https://github.com/liliana-sanfilippo/bibtex-ts-parser/issues"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.113",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "assert": "^2.1.0",
    "babel-loader": "^8.4.1",
    "buffer": "^6.0.3",
    "chai": "^4.3.6",
    "glob": "^8.0.3",
    "mocha": "^10.8.2",
    "process": "^0.11.10",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "5.2",
    "webpack": "5.94.0",
    "webpack-cli": "^4.10.0",
    "webpack-license-plugin": "^4.5.1"
  }
}
